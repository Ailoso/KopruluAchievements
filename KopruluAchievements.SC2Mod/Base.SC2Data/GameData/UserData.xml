        <Fields Id="Title" Type="Text" EditorColumn="3"/>
        <Fields Id="Description" Type="Text" EditorColumn="4"/>
        <Fields Id="Points" Type="Int" EditorColumn="5"/>
        <Fields>
            <Id value="Race"/>
            <Type value="GameLink"/>
            <GameLinkType value="Race"/>
            <EditorColumn value="6"/>
