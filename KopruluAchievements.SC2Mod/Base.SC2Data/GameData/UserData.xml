            </GameLink>
            <Image Image="Assets\Textures\btn-ability-protoss-entomb.dds">
                <Field Id="Icon"/>
            </Image>
            <Int Int="10">
                <Field Id="Points"/>
            </Int>
            <Int Int="1">
                <Field Id="Category"/>
            </Int>
            <String String="TIns00">
                <Field Id="ID"/>
            </String>
            <User Type="Rewards" Instance="[Default]">
                <Field Id="RewardCache"/>
